  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "server/*"
    ],
    "ignore": [
      "*.spec.ts *./artifacts/**"
    ],
    "ext": "ts,js,json",
    "exec": "ts-node ./server/app.ts"
  }
